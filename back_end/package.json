  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^1.12.3",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "react-flip-move": "^3.0.4",
    "react-router-dom": "^5.2.0",
    "sfcookies": "^1.0.2"
  }
}
